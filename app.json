{
  "expo": {
    "name": "task-app",
    "slug": "task-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "task-app",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#161622"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "permissions": [
        "android.permission.RECEIVE_BOOT_COMPLETED",
        "android.permission.SCHEDULE_EXACT_ALARM",
        "android.permission.POST_NOTIFICATIONS",
        "android.permission.VIBRATE"
      ],
      "package": "com.azeez.taskapp",
      "splash": {
        "backgroundColor": "#161622",
        "resizeMode": "contain",
        "image": "./assets/images/splash.png"
      },
      "googleServicesFile": "./google-services.json"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      ["expo-router"],
      [
        "expo-image-picker",
        {
          "photosPermission": "The app needs access to your photos to let you share them."
        }
      ],
      "expo-splash-screen",
      [
        "expo-build-properties",
        {
          "android": {
            "compileSdkVersion": 33,
            "targetSdkVersion": 33,
            "buildToolsVersion": "33.0.0",
            "extraMavenRepos": [
              "../../node_modules/@react-native-firebase/app/android/libs"
            ]
          }
        }
      ],
      [
        "@react-native-firebase/app",
        {
          "android_package_name": "com.azeez.taskapp",
          "google_services_file": "./google-services.json"
        }
      ],
      [
        "@react-native-firebase/messaging",
        {
          "auto_init": true,
          "android_package_name": "com.azeez.taskapp"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "eas": {
        "projectId": "b758d156-040c-4a13-a1a7-7a7fffe1566d"
      }
    }
  }
}
